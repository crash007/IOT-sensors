extends layout
block append head

block content
	script.
		 chartName = "#{chartName}"
	div(class="container")
		.panel.panel-default
			.panel-heading
				h3.panel-title Chart
			.panel-body
				.chart-container
		
				p
					a(href="/sensor/#{chartName}/json") View json
		.panel.panel-default
			.panel-heading
				h3.panel-title Location
			.panel-body
				#map-canvas(style="height: 20em; margin: 0px; padding: 0px; width:40em")
		//Photo panel
		.panel.panel-default
			.panel-heading
				h3.panel-title Photos
			.panel-body
				.photo-container		
        
block scripts

	script(src="/javascripts/highcharts.js")
	script(src="/javascripts/sensor-chart.js")
	script(src="https://maps.googleapis.com/maps/api/js?v=3.exp&signed_in=true")
