extends layout
block append head
//link(href="/stylesheets/c3.min.css", rel="stylesheet")

block content
	.container.body
		.panel.panel-default
			.panel-heading
				h3.panel-title Photos
			.panel-body
				form(action='/photos/upload',method="post",enctype="multipart/form-data")
					| Select images:
					input(type='file', name='photos',multiple="multiple")
					input(type='submit')
		section#wrapper
			article

		
		

block scripts
	script(type='text/javascript', src='http://maps.google.com/maps/api/js?sensor=true')
	script(src="/javascripts/location.js")
